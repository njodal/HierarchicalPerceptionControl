general:
  name: Tests for signals.py

  tests:
    - test:
        call: test_delayed_signal
        cases:
          - case:
              input:  [4, [1, 2, 3, 4, 5, 6, 7, 8]]
              output: 4
          - case:
              input:  [4, [1, 2, 3, 4]]
              output: 'None'
          - case:
              input:  [1, [1, 2, 3, 4, 5, 6, 7, 8]]
              output: 7
          - case:
              input:  [0, [1, 2, 3, 4, 5, 6, 7, 8]]
              output: 8
